        src="./images/dummy.png"
        data-src="/assets/images/backgrounds/lost-forest-3.png"
        alt="background"
    />
    <game-data class="data">
        <game-data-action type="enter">
            <game-logic type="once">
                <game-logic-action type="output">
                    This looks like a dead end.
                    I can see only trees and bushes.
                    No more path in that direction.
                </game-logic-action>
            </game-logic>
        </game-data-action>
    </game-data>
    <load src="/src/html/objects/lost-forest-3/lost-forest.xml"/>
    <load src="/src/html/objects/lost-forest-3/bird-1.xml"/>
    <load src="/src/html/objects/lost-forest-3/bird-2.xml"/>

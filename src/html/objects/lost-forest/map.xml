<!DOCTYPE game-interactable SYSTEM "http://www.game-object.com/dtd/interactable">
<game-interactable
    id="lost-forest/map"
    class="object"
    cx="41%"
    cy="73%"
    width="10%"
>
    <game-data class="data">
        <game-data-action type="inspect">
            <game-logic type="first">
                <game-logic-condition key="$has_map" value="TRUE">
                    <game-logic-action type="output">
                        Looks like a piece of the same map that i have with me ... just not in a good shape.
                    </game-logic-action>
                </game-logic-condition>
                <game-logic type="once">
                    <game-logic-action type="output">
                        It is very dirty and old, it is - hard - to identify anything.
                        But it looks like there are some huts in the area.
                    </game-logic-action>
                </game-logic>
                <game-logic-action type="output">
                    A map showing some huts in the area, i should look for them.
                </game-logic-action>
            </game-logic>
        </game-data-action>
    </game-data>
    <img
            class="image"
            src="./images/dummy.png"
            data-src="/assets/images/objects/footprints/footprints.png"
            alt="Continue Button"
    />
    <object
            class="click-area"
            data="./images/dummy.png"
            data-data="/assets/images/objects/footprints/footprints.svg"
            type="image/svg+xml"
    > </object>
</game-interactable>

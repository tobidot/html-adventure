<!DOCTYPE game-interactable SYSTEM "http://www.game-object.com/dtd/interactable">
<game-interactable
    id="lost-forest-3/bird-1"
    class="object"
    cx="29%"
    cy="34%"
    width="8%"
>
    <game-data class="data">
        <game-data-action type="interact">
            <game-logic type="all">
                <game-logic-action type="output">
                    [Bird screams]
                </game-logic-action>
                <game-logic-action type="set-attribute" key="#lost-forest-3\/bird-1@cy" value="-10%"> </game-logic-action>
                <game-logic-action type="output">
                    [Bird flies away]
                </game-logic-action>
            </game-logic>
        </game-data-action>
        <game-data-action type="inspect">
            <game-logic type="sequence">
                <game-logic-action type="output">
                    A bird. Wonder if it is the reason, why I feel like being watched.
                </game-logic-action>
            </game-logic>
        </game-data-action>
    </game-data>
    <img
            class="image"
            src="./images/dummy.png"
            data-src="/assets/images/objects/raven/raven.png"
            alt="Bird"
    />
    <object
            class="click-area"
            data="./images/dummy.png"
            data-data="/assets/images/objects/raven/raven.svg"
            type="image/svg+xml"
    > </object>
</game-interactable>

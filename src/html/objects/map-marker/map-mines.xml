<!DOCTYPE game-interactable SYSTEM "http://www.game-object.com/dtd/interactable">
<game-interactable id="map-mines" class="object map-object">
    <game-data class="data">
        <game-data-action type="interact">
            <game-logic-action type="change-scene" value="scene-mines"></game-logic-action>
        </game-data-action>
        <game-data-action type="inspect">
            <game-logic-action type="output">
                The mines, where most people of the area work at.
            </game-logic-action>
        </game-data-action>
    </game-data>
    <object
            class="click-area"
            data="./images/map/map-mines.svg"
            type="image/svg+xml"
    ></object>
</game-interactable>
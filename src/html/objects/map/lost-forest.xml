<!DOCTYPE game-interactable SYSTEM "http://www.game-object.com/dtd/interactable">
<game-interactable id="map/lost-forest" class="object map-object">
    <game-data class="data">
        <game-data-action type="interact">
            <game-logic-action type="change-scene" value="scene-lost-forest"> </game-logic-action>
        </game-data-action>
        <game-data-action type="inspect">
            <game-logic-action type="output">
                A path that leads away from the camp deeper into the forest.
            </game-logic-action>
        </game-data-action>
    </game-data>
    <object
            class="click-area"
            data="./images/dummy.png"
data-data="/public/images/map/map/lost-forest.svg"
            type="image/svg+xml"
    > </object>
</game-interactable>